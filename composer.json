{
    "name": "tomahawk/framework",
    "description": "The Tomahawk PHP Framework.",
    "keywords": ["framework", "tomahawk"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Ellis",
            "email": "tellishtc@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin",
        "process-timeout": 10000,
        "github-protocols": ["https"]
    },
    "require": {
        "php": ">=5.3.3",
        "ext-mcrypt": "*",
        "ext-mbstring": "*",
        "illuminate/database": "4.1.30",
        "classpreloader/classpreloader": "1.0.0",
        "jeremeamia/superclosure": "1.0.2",
        "doctrine/orm": "2.4.8",
        "doctrine/cache": "1.4.2",
        "ircmaxell/password-compat": "1.0.4",
        "filp/whoops": "1.0.7",
        "monolog/monolog": "1.12.0",
        "nesbot/carbon": "1.20.0",
        "patchwork/utf8": "1.1.28",
        "swiftmailer/swiftmailer": "5.2.2",
        "symfony/browser-kit": "2.7.9",
        "symfony/console": "2.7.9",
        "symfony/config": "2.7.9",
        "symfony/class-loader": "2.7.7",
        "symfony/css-selector": "2.7.7",
        "symfony/debug": "2.7.9",
        "symfony/dom-crawler": "2.7.9",
        "symfony/event-dispatcher": "2.7.9",
        "symfony/finder": "2.7.9",
        "symfony/http-foundation": "2.7.9",
        "symfony/http-kernel": "2.7.9",
        "symfony/process": "2.7.9",
        "symfony/routing": "2.7.9",
        "symfony/security-core": "2.7.9",
        "symfony/translation": "2.7.9",
        "symfony/templating": "2.7.9",
        "twig/twig": "~1.23.0",
        "phpseclib/phpseclib" : "1.0.0",
        "predis/predis" : "1.0.0",
        "psr/log" : "~1.0.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.6.*"
    },
    "autoload": {
        "psr-4": {
            "Tomahawk\\": "src/Tomahawk/"
        }
    },
    "minimum-stability": "stable"
}
