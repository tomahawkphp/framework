{
    "name": "tomahawk/commandbus",
    "description": "The Tomahawk PHP Command Bus Component.",
    "keywords": ["tomahawk", "commandbus"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Ellis",
            "email": "tellishtc@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin",
        "process-timeout": 10000,
        "github-protocols": ["https"]
    },
    "require": {
        "php": ">=5.6.4"

    },
    "require-dev": {
        "phpunit/phpunit": "~5.3.0"
    },
    "autoload": {
        "psr-4": {
            "Tomahawk\\CommandBus\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
